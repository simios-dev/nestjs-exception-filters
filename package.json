{
  "name": "@simios-dev/nestjs-exception-filters",
  "version": "1.0.0",
  "description": "Simios Exception Filter",
  "author": "Simi.OS.dev <simi.os.dev@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/test.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/simios-dev/nestjs-exception-filters"
  },
  "bugs": "https://github.com/simios-dev/nestjs-exception-filters",
  "peerDependencies": {
    "@nestjs/common": "^7.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@nestjs/common": "^7.3.2",
    "@nestjs/core": "^7.3.2",
    "@nestjs/platform-express": "^7.3.2",
    "@nestjs/testing": "7.3.2",
    "@types/express": "4.17.7",
    "@types/jest": "26.0.4",
    "@types/node": "14.0.22",
    "@types/supertest": "2.0.10",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "6.6.0",
    "supertest": "4.0.2",
    "ts-jest": "26.1.1",
    "ts-node": "8.10.2",
    "tsc-watch": "4.2.9",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.2",
    "typescript": "3.9.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
